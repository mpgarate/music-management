#! rockbox/tagbrowser/2.0

# Custom tagnavi config generated from Roon tags
# This creates a menu structure: [Tag List] -> [Album List]

# Tag selection menu
%menu_start "tag_selection" "Select Tag"
"*to-listen" ==> "tag_to_listen"
"*Top Albums" ==> "tag_top_albums"
"Alice" ==> "tag_alice"
"car" ==> "tag_car"
"classical" ==> "tag_classical"
"coding" ==> "tag_coding"
"dinner-vibes" ==> "tag_dinner_vibes"
"erhu" ==> "tag_erhu"
"fusion" ==> "tag_fusion"
"heavy" ==> "tag_heavy"
"jazz" ==> "tag_jazz"
"learning" ==> "tag_learning"
"to-delete" ==> "tag_to_delete"
"world" ==> "tag_world"
%menu_end

# Tag: *to-listen -> Albums with tracks in order
%menu_start "tag_to_listen" "*to-listen"
"*" -> album ? filename ~ "/beets-qobuz/The Album Leaf/2010 - A Chorus of Storytellers/" -> title = "fmt_title"
%menu_end

